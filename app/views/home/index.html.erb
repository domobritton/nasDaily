<head>
  <title>NasDaily</title>
  <meta name="description" content="1 Minute Videos. Daily.">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0 user-scalable=no">
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link rel="manifest" href="/manifest.json">
  <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="theme-color" content="#ffffff">

  <%= render partial: 'home/analytics' %>
  <%= javascript_pack_tag 'application' %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <%= render partial: 'home/facebook_sdk' %>
  <%= tag('div', id: 'fb-app-id', data: ENV.fetch('FB_APP_ID'))%></div>
  <%= tag('div', id: 'initial-videos-data', data: @videos)%></div>
</body>
